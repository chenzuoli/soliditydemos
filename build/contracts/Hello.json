  "source": "pragma solidity ^0.5.8;\n\ncontract Hello {\n    function say() pure public returns(string memory) {\n        return \"Hello World\";\n    }\n\n    function sum(uint a, uint b) pure public returns(uint val) {\n        val = a + b;\n        return val;\n    }\n}",
